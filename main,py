import yaml

def load_yaml(file_path):
    with open(file_path, 'r') as file:
        try:
            data = yaml.safe_load(file)
            return data
        except yaml.YAMLError as e:
            print(f'Błąd parsowania pliku YAML: {e}')
            return None

file_path = 'plik.yml'
data = load_yaml(file_path)
if data:
    # Przetwarzanie danych
    print(data)
